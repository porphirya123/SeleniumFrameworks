package JavaConcepts;

public class AbstractClassDemo extends Demo{

	@Override
	public void printmethod() {

		System.out.println("Print Abstract Class Method");
	}

	@Override
	public void sum() {
		
		int a=20;
		int b=40;
		System.out.println("Sum = "+a+b);
	}
	
	public static void main(String[] args)
	{
		AbstractClassDemo obj = new AbstractClassDemo();
		obj.printmethod();
		obj.sum();
	}

}


abstract class Demo
{

	public abstract void printmethod();
	
	public abstract void sum();
	
	
}

class Call extends AbstractClassDemo
{
	public static void main()
	{
		Call call = new Call();
		call.printmethod();
	}
}

