package JavaConcepts;

public class MathTrics {

	
	public void reverseNum()

	{
		int num = 123455;
		int rev = 0;
		while(num!=0)
		{
			int rem = num%10;
			rev = rev*10+rem;
			num = num/10;
			
		}System.out.println(rev);
	}
	
	public static void main(String[] args) {
		MathTrics obj = new MathTrics();
		obj.reverseNum();
	}
	
}
