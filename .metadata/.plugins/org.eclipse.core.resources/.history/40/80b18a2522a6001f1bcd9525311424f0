package JavaConcepts;

public class ForLoopPractice {
	
	
	public void printstar()
	{
		for(int i=0;i<=5;i++)
		{
			for(int j=5;j>=0;j--)
			{
				System.out.print("*");
			}
		}
	}
	
	
	
	
	/** Method Insertion */
	
	public static void Loop(int num)
	{
		if(num<=80)
		{
			System.out.println(num);
			Loop(num+1);
		}
	}
	
	
	
		
	/** For Loop logic */
	
	
	
	
	public static void main(String[] args)

	{
		ForLoopPractice obj = new ForLoopPractice();
		obj.printstar();
	}
	
}
